<script>
$(document).ready(function() {
    // Get the current time
    var currentTime = new Date().getTime();

    // Check if the last shown time is stored in localStorage
    var lastPopupTime = localStorage.getItem('lastPopupTime');

    // If there's no time stored or more than 24 hours have passed, clear the storage and show the popup
    if (!lastPopupTime || currentTime - lastPopupTime > 24 * 60 * 60 * 1000) {
        localStorage.clear(); // Clear all stored data
        setTimeout(function() {
            $('#bookingModal').addClass('active');
        }, 1000); // Show modal after 1 second delay

        // Update the last popup time in localStorage
        localStorage.setItem('lastPopupTime', currentTime);
    }

    // Close modal when close button is clicked
    $('#closeModalBtn').click(function() {
        $('#bookingModal').removeClass('active');
    });

    // Handle Book Now button click
    $('#bookNowBtn').click(function() {
        window.location.href = '/booking';
    });

    // Close modal when clicking outside
    $('#bookingModal').click(function(e) {
        if (e.target === this) {
            $(this).removeClass('active');
        }
    });
});

</script>

